page{
  font-size: 30rpx;
  height: 100%;
}
.container {
  height: 100%;
}
/* 头部head全局样式 */
.head{
  height: 180rpx;
  background-color: #1a76d1;
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  box-sizing: border-box;
  padding: 0 30rpx;
}
.head image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 100rpx;
  margin-right: 25rpx;
}
.head .head-text{
  display: flex;
  flex-direction: column;
  width: 550rpx;
}
.head .head-text .user-name{
  width: 100%;
 color: #fff;
 overflow: hidden;
 white-space:nowrap;
 text-overflow: ellipsis;
}
.noMore{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  z-index: -999;
 }
 .noMore image{
   width:300rpx;
   height: 300rpx;
   display: block;
   margin: 50% auto auto auto;
 }
 .noMore .kongkong{
   margin-top: 25rpx;
   display: block;
   text-align: center;
   color: rgb(184, 184, 184);
   font-size: 35rpx;
 }